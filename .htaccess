RewriteEngine On
RewriteCond %{HTTP_HOST} http://dharmendrastudiorentals.castindia.in [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ http://dharmendrastudiorentals.castindia.in/$1 [NC,R,L]
RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA]